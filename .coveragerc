[run]
branch = True
source = eqcorrscan
omit =
  eqcorrscan/__init__.py
  eqcorrscan/*/__init__.py
  eqcorrscan/core/match_filter_internal.py
  eqcorrscan/tests/*
  eqcorrscan/utils/gui/*
  eqcorrscan/utils/timer.py
  eqcorrscan/utils/plotting.py
  eqcorrscan/utils/sfile_util.py
[report]
exclude_lines =
    pragma: no cover
    def __repr__
    if self\.debug
    raise AssertionError
    raise NotImplementedError
    if 0:
    if __name__ == __main__:
ignore_errors = True